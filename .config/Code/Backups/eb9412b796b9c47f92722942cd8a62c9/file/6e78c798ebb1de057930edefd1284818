setInterval(changeColor, 1000);
     function changeColor() {
      var x=document.getElementById("title");
      if(x.style.color=="green")
       x.style.color="silver";
      else if (x.style.color=="green")
       x.style.color="silver";
      else if (x.style.color=="green")
       x.style.color="silver";
      else x.style.color="green";
     } 